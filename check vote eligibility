#include <iostream>
using namespace std;
int main() {
int age;
bool isCitizen;
// Input age and citizenship status
cout << "Enter age: ";
cin >> age;
cout << "Are you a citizen? Enter 1 for Yes or 0 for No: ";
cin >> isCitizen;
// Check voting eligibility
if (age >= 18 && isCitizen) {
cout << "You are eligible to vote." << endl;
} else {
cout << "You are not eligible to vote." << endl;
}
return 0;
}
